Description='A wireless connection using a custom network block configuration'
Interface=wlp3s0
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
    'ssid="eduroam"'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'group=CCMP TKIP'
    'pairwise=CCMP'
    'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.p11-kit"'
    'identity=""'
    'password=""'
    'phase2="auth=MSCHAPV2"'
)
